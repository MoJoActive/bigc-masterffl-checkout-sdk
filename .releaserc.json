{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    "next",
    "next-major",
    { "name": "beta", "prerelease": true },
    { "name": "alpha", "prerelease": true }
  ],
  "plugins": [
    ["@semantic-release/commit-analyzer", { "preset": "conventionalcommits" }],
    ["@semantic-release/release-notes-generator", { "preset": "conventionalcommits" }],
    ["@semantic-release/changelog", { "changelogFile": "CHANGELOG.md" }],
    ["@semantic-release/npm", { "npmPublish": false }],
    ["@semantic-release/git", { "assets": ["CHANGELOG.md", "package.json", "package-lock.json"], "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}" }],
    ["@semantic-release/github", { "assets": [
      { "path": "dist/bigc-masterffl-sdk.checkout.es.js", "label": "ESModule build" },
      { "path": "dist/bigc-masterffl-sdk.checkout.umd.cjs", "label": "UMD build" },
      { "path": "dist/bigc-masterffl-sdk.standalone.js", "label": "Standalone build" }
    ] }]
  ]
}
